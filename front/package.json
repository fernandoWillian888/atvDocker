{
    "version": "1.0",
    "build": {
      "from": "node:14-alpine",
      "workdir": "/atvdocker",
      "copy": [
        {
          "source": "package.json",
          "destination": "./"
        },
        {
          "source": ".",
          "destination": "."
        }
      ],
      "run": [
        "npm run build"
      ],
      "expose": 3000,
      "cmd": ["npm", "start"]
    }
  }
  